    "eslint-plugin-prettier": "^5.1.3",
    "openapi-types": "^12.1.3",
    "rollup": "^4.13.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.6.2",
    "typescript": "^5.1.3"
